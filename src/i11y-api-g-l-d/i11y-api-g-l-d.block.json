{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "myprefix/i11y-api-g-l-d",
  "version": "0.1.0",
  "title": "WP Interactivity API Demo",
  "category": "widgets",
  "icon": "lightbulb",
  "description": "Demonstrate the WP Interactivity API using global state, local context, and derived state",
  "example": {},
  "attributes": {
    "productPrice": {
      "type": "number",
      "default": 1
    },
    "productName": {
      "type": "string",
      "default": "Product Name"
    }
  },
  "supports": {
    "interactivity": true,
    "align": ["full", "wide"]
  },
  "textdomain": "textDomain",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style.css",
  "render": "file:./render.php",
  "viewScriptModule": "file:./view.js"
}
