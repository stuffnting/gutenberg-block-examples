{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "myprefix/variations-register-block",
	"title": "Variations Register Block",
	"description": "A block with variations that allow for initial attribute values to be set",
	"icon": "lightbulb",
	"category": "text",
	"supports": {
		"color": {
			"text": true,
			"background": true
		}
	},
	"attributes": {
		"isActiveName": {
			"type": "string",
			"default": ""
		},
		"content": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"placeholder": {
			"type": "string"
		},
		"textAlign": {
			"type": "string"
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style.css"
}
