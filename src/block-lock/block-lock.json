{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "myprefix/block-lock",
  "title": "Block Lock",
  "icon": "lightbulb",
  "category": "text",
  "description": "Experiment with the block lock.",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2",
      "default": ""
    },
    "lock": {
      "_NOTE": "Using this attribute sets an initial value for the lock, the Option menu lock will change this value.",
      "type": "object",
      "default": {
        "move": true,
        "remove": true
      }
    }
  },
  "supports": {
    "lock": true
  },
  "editorScript": "myprefix-block-lock"
}
