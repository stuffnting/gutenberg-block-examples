{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "myprefix/dynamic-render-php",
  "title": "Render PHP Template",
  "icon": "lightbulb",
  "category": "text",
  "description": "Test rendering with PHP template file",
  "keywords": ["heading", "test"],
  "textdomain": "textDomain",
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2",
      "default": ""
    }
  },
  "example": {
    "attributes": {
      "content": "Tester McTest!"
    }
  },
  "editorScript": "file:./index.js",
  "render": "file:./render.php"
}
